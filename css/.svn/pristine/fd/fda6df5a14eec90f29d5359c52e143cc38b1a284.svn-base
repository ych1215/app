.width_150px { width:150px; }
.width_100px { width:100px; }

/* height */
.height_100p { height:100%; }
.height_90p { height:90%; }
.height_80p { height:80%; }
.height_70p { height:70%; }
.height_60p { height:60%; }
.height_50p { height:50%; }
.height_40p { height:40%; }
.height_30p { height:30%; }
.height_20p { height:20%; }
.height_10p { height:10%; }

.height_400px { height:400px; }
.height_200px { height:200px; }
