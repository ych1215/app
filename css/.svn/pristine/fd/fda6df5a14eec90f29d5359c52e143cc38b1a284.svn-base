/* ========== [옵션] ========== */
.display_none { display:none; }
.display_inline { display:inline; }
.display_block { display:block; }

.clickable { cursor:pointer; }
.noclickable { cursor:default; }

/* align 시리즈 속성 정의 */
.left {text-align:left !important;}
.center {text-align:center !important;}
.right {text-align:right !important;} 

/* clearfix */
.clearfix:after { content:"."; display:block; height:0; clear:both; visibility:hidden; }
.clearfix { display:inline-block; }
/* Hides from IE-mac \*/
*html .clearfix {height: 1%;}
.clearfix {display: block;}
/* End hide from IE-mac */

/* float */
.float_l { float:left; }
.float_r { float:right; }
.clear_b { clear:both !important;}


/* 글씨& 폰트 */
.txt_red { color:#e51937; }
.txt_orange { color:#f58026; }

.txt_tline {text-decoration:line-through}

span.mid { display:inline-block; padding:7px 3px; font-size:14px; font-weight:normal; text-align: center; }


/* ========== [버튼 & 달력] ========== */
/* disabled */
.btn_big.disabled,
.btn_big[disabled],
.btn_big[disabled]:hover,
.btn_big[disabled]:active,
.btn_big[disabled]:focus,
.btn_mid.disabled,
.btn_mid[disabled],
.btn_mid[disabled]:hover,
.btn_mid[disabled]:active,
.btn_mid[disabled]:focus,
.btn_inner.disabled,
.btn_inner[disabled],
.btn_inner[disabled]:hover,
.btn_inner[disabled]:active,
.btn_inner[disabled]:focus,
.btn_bbs_top.disabled,
.btn_bbs_top[disabled],
.btn_bbs_top[disabled]:hover,
.btn_bbs_top[disabled]:active,
.btn_bbs_top[disabled]:focus { cursor:default; color: #cccccc; background-color: #858e98; border: 1px solid #cccccc; background-image:none; }
.btn_bbs_type.disabled,
.btn_bbs_type[disabled],
.btn_bbs_type[disabled]:hover,
.btn_bbs_type[disabled]:active,
.btn_bbs_type[disabled]:focus { cursor:default; color: #cccccc; background-color: #858e98; background-position:-180px 9px;}


/* datepicker-trigger */
.ui-monthpicker-trigger,
.ui-datepicker-trigger,
button.ui-monthpicker-trigger,
button.ui-datepicker-trigger{
    display: inline-block;
	margin: 0 0 0 5px;
	padding:0;
    width: 32px; height:32px;
	vertical-align: bottom;
	background: url('/resources/images/common/ico_m_calendar_on.png') 0 0 no-repeat;
	background-size: 37px 37px;
	background-position: -3px -3px;
    border: none;
}
.ui-monthpicker-disabled,
.ui-datepicker-disabled,
button[disabled].ui-monthpicker-trigger,
button[disabled].ui-datepicker-trigger {
    display: inline-block;
    margin: 0 0 0 5px;
	padding:0;
    width: 32px; height:32px;
	vertical-align: bottom;
    background: url('/resources/images/common/ico_m_calendar_off.png') 0 0 no-repeat;
    background-size: 37px 37px;
    background-position: -3px -3px;
    border: none;
}

.ui-datepicker td span,
.ui-datepicker td a {padding: 0.44em;}
.ui-datepicker .ui-datepicker-title select { padding: 6px; line-height: 18px;}
.ui-datepicker .ui-datepicker-prev span,
.ui-datepicker .ui-datepicker-next span {margin-top: -8px;}
.ui-datepicker .ui-datepicker-year,
.ui-datepicker .ui-datepicker-month {
    color:#333;
    border: 1px solid #aaa;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.3), 0 1px 1px rgba(255,255,255,.27);
    -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,.3), 0 1px 1px rgba(255,255,255,.27);
    box-shadow: inset 0 1px 1px rgba(0,0,0,.3), 0 1px 1px rgba(255,255,255,.27);
    outline: none;
    cursor: pointer;
    background:#f9f9f9;
}
.ui-datepicker .ui-datepicker-prev, .ui-datepicker .ui-datepicker-next {
	top: 3px;
	width: 2.2em;
	height: 2.2em;
}
.ui-datepicker-prev:hover,
.ui-datepicker-next:hover { background:none; border: none; }
.ui-icon-circle-triangle-e {
	background-position:0 0;
}
.ui-icon.ui-icon-circle-triangle-e {
	width: 16px;
	height: 16px;
	background:url('/resources/images/common/ico_arrow_right.png') 0 0 no-repeat;
}
.ui-icon.ui-icon-circle-triangle-w {
	width: 16px;
	height: 16px;
	background:url('/resources/images/common/ico_arrow_left.png') 0 0 no-repeat;
}

/* radio & checkbox */
input[type="radio"],
input[type="checkbox"] {
	width: auto;
	line-height: normal;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
}
.radio,
.checkbox {
	display: inline-block;
	vertical-align: bottom;
	min-height: 18px;
	margin:0;
	padding:0;
}
.radio + .radio,
.checkbox + .checkbox {
	margin-left: 10px;
}
.controls .radio:first-child,
.controls .checkbox:first-child {
	margin-left: 0px;
}
.checkbox input[type="checkbox"],
.radio input[type="radio"] {
	position: absolute;
	left: -100000px;
}
/* checkbox */
.checkbox span {
	display: inline-block;
	margin:0 5px 0 0; padding: 0;
    width: 20px; height: 20px;
    cursor: pointer;
    vertical-align: top;
}

.checkbox span:before {
	content: "";
	display: inline-block;
	margin: 0 5px 0 0; padding: 0;
	width: 18px; height: 18px;
    background:url('/resources/images/common/radio_check.png') no-repeat;
    background-position: 0 0;
    background-size:auto 18px;
    border:1px solid #ccc;
}
.checkbox span.disabled {cursor:default;}
.checkbox span.disabled:before {
	background-position: -40px 0;
	background-size:auto 18px;
}
.checkbox span.check:before {
	background-position: -80px 0;
	background-size:auto 18px;
}
.checkbox span.check.disabled:before {
    background:url('/resources/images/common/radio_check.png') no-repeat;
    background-position: -60px 0;
    background-size:auto 18px;
}
input:checked + .inp-check:before {
    background:url('/resources/images/common/radio_check.png') no-repeat;
    background-position: -20px 0;
    background-size:auto 18px;
}
input:checked + .inp-check.disabled:before {
    background:url('/resources/images/common/radio_check.png') no-repeat;
    background-position: -60px 0;
    background-size:auto 18px;
}

/* radio */
.radio span {
	display: inline-block;
	margin: 0 5px 0 0; padding: 0;
	width: 20px; height: 20px;
	cursor: pointer;
    vertical-align: top;
}
.radio span:before {
	content: "";
	display: inline-block;
	margin: 0 5px 0 0; padding: 0;
	width:18px; height: 18px;
	background:url('/resources/images/common/radio_check.png') no-repeat;
    background-position: -100px 0;
    background-size:auto 18px;
    border:1px solid #ccc;
	border-radius: 9px; 
}
.radio span.disabled {cursor:default;}
.radio span.disabled:before { 
    background-position: -140px 0;
    background-size:auto 18px;
}
.radio span.check:before { 
    background-position: -120px 0;
    background-size:auto 18px;
}
.radio span.check.disabled:before { 
    background-position: -160px 0;
    background-size:auto 18px;
}
input:checked + .inp-radio:before {
    background:url('/resources/images/common/radio_check.png') no-repeat;
	background-position: -120px 0;
	background-size:auto 18px;
}
input:checked + .inp-radio.disabled:before {
	background:url('/resources/images/common/radio_check.png') no-repeat;
	background-position: -160px 0;
	background-size:auto 18px;
}

label.radio + label.radio,
label.checkbox + label.radio,
label.radio + label.checkbox {margin-left:5px;}

